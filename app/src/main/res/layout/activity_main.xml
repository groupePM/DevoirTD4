<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context=".TD4Activity"
        tools:layout_editor_absoluteX="-160dp"
        tools:layout_editor_absoluteY="-53dp">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="776dp"
            android:layout_height="1409dp"
            android:layout_alignParentTop="true"
            android:orientation="horizontal">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/button" />

            <TextView
                android:id="@+id/lnumberT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/launches"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="init"
                android:text="@string/init" />

            <FrameLayout
                android:layout_width="50dp"
                android:layout_height="399dp"></FrameLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

            </LinearLayout>

            <FrameLayout
                android:layout_width="200dp"
                android:layout_height="0dp"></FrameLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            </LinearLayout>

            <LinearLayout
                android:layout_width="34dp"
                android:layout_height="879dp"
                android:orientation="horizontal">

            </LinearLayout>

            <FrameLayout
                android:layout_width="50dp"
                android:layout_height="100dp"
                android:layout_weight="10"></FrameLayout>

            <TextView
                android:id="@+id/levelV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/one"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <TextView
                android:id="@+id/scoreV"
                android:layout_width="83dp"
                android:layout_height="412dp"
                android:text="@string/zero"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <TextView
                android:id="@+id/levelT"
                android:layout_width="851dp"
                android:layout_height="153dp"
                android:text="@string/level"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <TextView
                android:id="@+id/scoreT"
                android:layout_width="wrap_content"
                android:layout_height="460dp"
                android:text="@string/score"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <TextView
                android:id="@+id/lnumberV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/zero"
                android:textAppearance="?android:attr/textAppearanceLarge" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true">

        </FrameLayout>

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>